{
  "name": "skeleton-components",
  "version": "1.0.0",
  "description": "A front-end template build upon Rosid",
  "private": true,
  "authors": [
    "Tobias Reich <tobias@electerious.com>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/electerious/Skeleton-Components.git"
  },
  "license": "MIT",
  "scripts": {
    "start": "rosid serve src/ -o -s '/ui/index.html'",
    "compile": "rosid compile src/ dist/ -i '**/_*' -i '**/*.{data,config}.{js,json}'",
    "preview": "npm run compile && rsync --recursive --delete --checksum --whole-file --verbose --progress --human-readable dist/ comwrap-deployment@preview.comwrap.rocks:/srv/www/preview.comwrap.rocks/htdocs/malvid/"
  },
  "dependencies": {
    "basicgrid": "^3.1.2",
    "basicslider": "^1.1.3",
    "normalize.css": "^8.0.0",
    "rosid": "^10.0.0",
    "rosid-handler-js": "^12.1.0",
    "rosid-handler-malvid": "^3.0.7",
    "rosid-handler-njk": "^7.2.0",
    "rosid-handler-sass": "^6.0.4"
  }
}
