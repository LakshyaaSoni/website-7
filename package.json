{
  "name": "skeleton-components",
  "version": "1.0.0",
  "description": "A front-end template build upon Rosid",
  "private": true,
  "authors": [
    "Tobias Reich <tobias@electerious.com>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/electerious/Skeleton-Components.git"
  },
  "license": "MIT",
  "scripts": {
    "start": "rosid serve src/ -o -s '/ui/index.html'",
    "compile": "rosid compile src/ dist/ -i '**/_*' -i '**/*.{data,config}.{js,json}'",
    "preview": "npm run compile && rsync --recursive --delete --checksum --whole-file --verbose --progress --human-readable dist/ frontend@preview.comwrap.rocks:/var/www/preview/malvid/"
  },
  "dependencies": {
    "basicgrid": "^3.0.0",
    "normalize.css": "^7.0.0",
    "rosid": "^9.0.1",
    "rosid-handler-js": "^10.1.0",
    "rosid-handler-malvid": "^1.0.0",
    "rosid-handler-njk": "^6.0.0",
    "rosid-handler-sass": "^5.0.2"
  }
}